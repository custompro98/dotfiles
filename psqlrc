# Set prompt format
\set PROMPT1 '%[%033[1;33m%]%M %n@%/%R%[%033[0m%]%# '
#\set PROMPT2 '[more] %R > '

# Make null visible
\pset null '[NULL]'

# Use table format unless too much data
\x auto

# Use separate history file per db
\set HISTFILE ~/.psql_history- :DBNAME

# Autocomplete keywords in upper-case
\set COMP_KEYWORD_CASE upper
